[machines]
${server-ip} fqdn=server.kubernetes.local host_short=server
${node-0-ip} fqdn=node-0.kubernetes.local host_short=node-0
${node-1-ip} fqdn=node-1.kubernetes.local host_short=node-1

[jumpbox]
localhost ansible_connection=local

[servers]
server ansible_host=${server-ip}

[nodes]
node-0 ansible_host=${node-0-ip}
node-1 ansible_host=${node-1-ip}

[all:vars]
ansible_user=admin
ansible_ssh_private_key_file=~/.ssh/key-us-east-1.pem